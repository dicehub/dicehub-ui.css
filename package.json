{
  "name": "dicehub-ui",
  "version": "0.1.0",
  "description": "Dicehub UI Framework",
  "main": "dist/dicehub-ui.min.css",
  "scripts": {
    "start:gulp": "npm run sprite:ui && gulp",
    "build:gulp": "npm run sprite:ui && gulp build --minify",
    "start:vuepress": "npm run sprite:docs && vuepress dev docs",
    "build:vuepress": "npm run sprite:docs && vuepress build docs -d dist_docs",
    "build": "npm run build:gulp",
    "sprite:docs": "svg2sprite src/icons docs/.vuepress/public/sprite.svg --iconPrefix=svg- --stripEmptyTags --stripExtraAttrs",
    "sprite:ui": "svg2sprite src/icons dist/sprite.svg --iconPrefix=svg- --stripEmptyTags --stripExtraAttrs"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.2.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-error-notifier": "^1.1.0",
    "gulp-if": "^2.0.2",
    "gulp-include": "^2.3.1",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-sass-glob": "^1.0.9",
    "gulp-watch": "^5.0.0",
    "minimist": "^1.2.0",
    "mkdir-p": "^0.0.7",
    "postcss-custom-properties": "^7.0.0",
    "svg2sprite-cli": "^2.0.1",
    "vuepress": "^0.13.0"
  },
  "dependencies": {}
}
